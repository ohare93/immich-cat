{
  "name": "elm-app",
  "version": "0.1.0",
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "start": "node scripts/start-dev.js",
    "build": "node scripts/build.js",
    "dev-secure": "node scripts/dev-server.js",
    "docker:build": "docker build -t immich-cat .",
