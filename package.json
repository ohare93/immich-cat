    "docker:rebuild": "docker-compose up --build"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "elm": "^0.19.1",
    "elm-live": "^4.0.2",
    "thumbhash": "^0.1.1"
  }
}
